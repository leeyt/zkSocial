<hbox width="100%">
	<!-- Menu Toggle Button -->
	<cell align="left" width="10%">
		<button
			sclass="button touchable"
			style="border: none" 
			image="data:image/png;base64,"
			onClick='@command("toggleMenu")' />
	</cell>
	<!--  -->
	
	<!-- Push Notifications -->
	<!-- Friend requests, Instant Messages, Notification Messages -->
	<cell align="center" valign="middle" width="90%">				
		<image
			sclass="touchable icon"
			src="data:image/png;base64,"
			onClick='Clients.showNotification("No Invitations", Clients.NOTIFICATION_TYPE_INFO, self, "after_center", 1000);' />
		<image
			sclass="touchable icon"
			src="data:image/png;base64,"
			onClick='Clients.showNotification("No Messages", Clients.NOTIFICATION_TYPE_INFO, self, "after_center", 1000);' />
		<image
			sclass="touchable icon"
			src="data:image/png;base64,"
			onClick='Clients.showNotification("No Notifications", Clients.NOTIFICATION_TYPE_INFO, self, "after_center", 1000)' />
	</cell>
	<!--  -->
</hbox>
